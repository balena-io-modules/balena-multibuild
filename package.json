{
  "name": "resin-multibuild",
  "version": "0.9.0",
  "description": "Build a docker composition",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf build",
    "prepublish": "npm run build && npm run lint",
    "precommit": "npm run lint && npm run build && npm run test",
    "lint": "resin-lint --typescript lib/*.ts typings/*.ts test/*.ts",
    "build": "npm run clean && tsc --project tsconfig.publish.json",
    "build:test": "npm run clean && tsc --project .",
    "docgen": "typedoc --ignoreCompilerErrors --out docs --name resin-multibuild --readme README.md lib/",
    "test": "npm run lint && npm run build:test && mocha build/test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io-modules/resin-multibuild.git"
  },
  "author": "Cameron Diver <cameron@resin.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io-modules/resin-multibuild/issues"
  },
  "homepage": "https://github.com/resin-io-modules/resin-multibuild#readme",
  "devDependencies": {
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "husky": "^0.14.3",
    "mocha": "^3.2.0",
    "resin-lint": "^1.5.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/bluebird": "3.5.18",
    "@types/dockerode": "2.5.5",
    "@types/lodash": "^4.14.87",
    "@types/tar-stream": "^1.6.0",
    "ajv": "^6.6.1",
    "bluebird": "^3.5.1",
    "docker-progress": "^3.0.1",
    "dockerode": "^2.5.5",
    "lodash": "^4.17.4",
    "resin-bundle-resolve": "^0.6.0",
    "resin-compose-parse": "^2.0.1",
    "resin-docker-build": "^0.7.1",
    "tar-stream": "^1.5.5",
    "tar-utils": "^1.0.2",
    "typed-error": "^3.0.0"
  }
}
