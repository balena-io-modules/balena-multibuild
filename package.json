{
  "name": "resin-multibuild",
  "version": "4.2.2",
  "description": "Build a docker composition",
  "main": "build/index.js",
  "files": [
    "build/*"
  ],
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf build",
    "prepublishOnly": "npm run build && npm run lint",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"{lib,test,typings}/**/*.ts\"",
    "lint": "resin-lint --typescript lib typings test",
    "build": "npm run clean && tsc --project tsconfig.publish.json && npm run copy-secrets",
    "build:test": "npm run clean && tsc --project . && npm run copy-secrets:test",
    "copy-secrets": "cp lib/build-secrets/Dockerfile* lib/build-secrets/*.sh build/build-secrets",
    "copy-secrets:test": "cp lib/build-secrets/Dockerfile* lib/build-secrets/*.sh build/lib/build-secrets",
    "docgen": "typedoc --ignoreCompilerErrors --out docs --name resin-multibuild --readme README.md lib/",
    "test": "npm run lint && npm run build:test && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io-modules/resin-multibuild.git"
  },
  "author": "Cameron Diver <cameron@resin.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io-modules/resin-multibuild/issues"
  },
  "homepage": "https://github.com/resin-io-modules/resin-multibuild#readme",
  "devDependencies": {
    "@types/chai-as-promised": "^7.1.0",
    "@types/js-yaml": "^3.12.1",
    "@types/mocha": "^5.2.6",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.3",
    "prettier": "^1.16.4",
    "resin-lint": "^3.0.2",
    "typedoc": "^0.9.0",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@types/bluebird": "3.5.20",
    "@types/dockerode": "2.5.5",
    "@types/lodash": "4.14.112",
    "@types/tar-stream": "1.6.0",
    "ajv": "^6.10.0",
    "bluebird": "^3.5.4",
    "docker-progress": "^4.0.0",
    "docker-toolbelt": "^3.3.7",
    "dockerfile-template": "^0.1.0",
    "dockerode": "^2.5.8",
    "fp-ts": "^2.0.0",
    "io-ts": "^2.0.0",
    "io-ts-reporters": "^1.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.4",
    "resin-bundle-resolve": "^4.1.3",
    "resin-compose-parse": "^2.0.4",
    "resin-docker-build": "^1.0.1",
    "tar-stream": "^2.0.1",
    "tar-utils": "^2.0.0",
    "typed-error": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
